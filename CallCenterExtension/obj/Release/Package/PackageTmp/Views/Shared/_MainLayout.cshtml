<!DOCTYPE html>
<html>
<head>

    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @*<script src="~/Scripts/jquery-3.3.1.js"></script>*@
</head>
<body>
    <div class="text-center">
        <h2 class="web-title"> JOB ORDER </h2>
        <div class="right-title">
            <h3 class="emp-info">@(ViewBag.EmpName + " (" + ViewBag.UserRole + ")")</h3>
            <label class="logout" id="logout">@Html.ActionLink("LogOut", "LogOut", "Login")</label>
        </div>
    </div>

    <div>
        @RenderBody()
    </div>
</body>
</html>


